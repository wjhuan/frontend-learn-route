{
  "name": "frontend-learn-route",
  "version": "1.0.0",
  "description": "分享自己的前端学习之路的一点一滴整理成文档，涵盖大部分前端开发者所需要掌握的核心知识，与大家共同成长！",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vuepress build docs",
    "commit": "git-cz"
  },
  "keywords": [
    "vue",
    "docs",
    "vuepress",
    "learning",
    "wangjiahuan"
  ],
  "author": "wangjiahuan",
  "license": "Apache License",
  "dependencies": {
    "ant-design-vue": "^1.6.4",
    "docsearch.js": "^2.5.2",
    "lodash": "^4.17.15",
    "core-js": "^2.6.11",
    "vuex": "^3.1.3"
  },
  "devDependencies":{
    "@vuepress/plugin-active-header-links": "^1.5.0",
    "@vuepress/plugin-nprogress": "^1.5.0",
    "@vuepress/plugin-search": "^1.5.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "prettier": "^2.0.5",
    "vuepress": "^1.5.2",
    "vuepress-plugin-container": "^2.1.2",
    "vuepress-plugin-smooth-scroll": "^0.0.9",
    "less": "^3.12.2",
    "less-loader": "^6.2.0"
  },
  "lint-staged": {
    "*.{vue,js,json,css,less}": [
      "node scripts/lint-prettier.js",
      "git add"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
